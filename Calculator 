      "cell_type": "code",
      "source": [
        "from ipywidgets import interact, widgets\n",
        "\n",
        "# Define calculator operation\n",
        "def calculator(op, x, y):\n",
        "    if op == '+':\n",
        "        return x + y\n",
        "    elif op == '-':\n",
        "        return x - y\n",
        "    elif op == '*':\n",
        "        return x * y\n",
        "    elif op == '/':\n",
        "        return x / y if y != 0 else \"‼️ Division by zero\"\n",
        "\n",
        "# Build interface\n",
        "ops = ['+', '-', '*', '/']\n",
        "interact(\n",
        "    calculator,\n",
        "    op=widgets.Dropdown(options=ops, description='Operation:'),\n",
        "    x=widgets.FloatText(value=3.0, description='X:'),\n",
        "    y=widgets.FloatText(value=3.0, description='Y:')\n",
        ");"
      ],
      "metadata": {
